<view class="content" style="{{'height:'+(imgHeight+'px')+';'}}"><image style="width:100%;position:absolute;bottom:0;height:100%;" src="../../static/login/backgroudimg.png"></image><image class="returnClass" src="../../static/login/back.png" data-event-opts="{{[['tap',[['returnClick',['$event']]]]]}}" bindtap="__e"></image><view class="inputContent"><view class="inputItem phoneNum"><image class="iconClass1" src="../../static/login/phone.png"></image><input class="inputClass" type="number" placeholder="手机号码" maxlength="11" data-key="phoneNumber" data-event-opts="{{[['input',[['inputChange1',['$event']]]]]}}" bindinput="__e"/></view><view class="inputItem Captcha"><image class="iconClass2" src="../../static/login/code.png"></image><input class="inputClass" type="number" placeholder="输入验证码" maxlength="6" data-key="captchaCode" data-event-opts="{{[['input',[['inputChange2',['$event']]]]]}}" bindinput="__e"/></view><view class="getCode style1" id="Code" data-event-opts="{{[['tap',[['getCodeClick',['$event']]]]]}}" bindtap="__e">{{textCode}}</view><image class="btnLogin" src="../../static/login/btnLogin.png"></image><text data-event-opts="{{[['tap',[['loginClick',['$event']]]]]}}" class="fontStyle" bindtap="__e">确定</text></view><image class="logoClass" src="../../static/login/logo.png"></image></view>